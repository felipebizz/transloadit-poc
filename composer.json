{
    "name": "xuplau/transloadit",
    "description": "Transloadit POC",
    "homepage": "https://github.com/ivanrosolen/transloadit-poc",
    "require": {
        "php" : ">=7.0",
        "transloadit/php-sdk": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit" : "5.5.*"
    },
    "autoload": {
        "psr-4": {
            "Xuplau\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Xuplau\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit tests"
    }
}
